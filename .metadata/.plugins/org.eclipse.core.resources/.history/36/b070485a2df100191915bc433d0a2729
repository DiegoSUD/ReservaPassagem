package model;

public class ReservaPassagem {
	
	private String flightNumber;
	private String passengers;
	private String Fligthclass;
	private String on;
	private String departingFrom;
	private String arrivingIn;
	private String orderNumber;
	
	
	public String getFlightNumber() {
		return flightNumber;
	}
	public void setFlightNumber(String flightNumber) {
		this.flightNumber = flightNumber;
	}
	public String getPassengers() {
		return passengers;
	}
	public void setPassengers(String passengers) {
		this.passengers = passengers;
	}
	public String getFligthclass() {
		return Fligthclass;
	}
	public void setFligthclass(String fligthclass) {
		Fligthclass = fligthclass;
	}
	public String getOn() {
		return on;
	}
	public void setOn(String on) {
		this.on = on;
	}
	public String getDepartingFrom() {
		return departingFrom;
	}
	public void setDepartingFrom(String epartingFrom) {
		this.departingFrom = epartingFrom;
	}
	public String getArrivingIn() {
		return arrivingIn;
	}
	public void setArrivingIn(String arrivingIn) {
		this.arrivingIn = arrivingIn;
	}
	public String getOrderNumber() {
		return orderNumber;
	}
	public String setOrderNumber(String orderNumber) {
		this.orderNumber = orderNumber;
	}
	

}
